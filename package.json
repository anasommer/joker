{
  "name": "joker",
  "version": "0.2.5",
  "description": "Application that tells jokes",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "npm run test-e2e",
    "test-e2e": "cypress open",
    "test-unit": "jest",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anasommer/joker.git"
  },
  "author": "Anastassia Sommer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anasommer/joker/issues"
  },
  "homepage": "https://github.com/anasommer/joker#readme",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "cypress": "^12.16.0",
    "eslint": "^8.43.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-jest": "^27.2.2",
    "husky": "^8.0.3",
    "jest": "^29.2.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8"
  },
  "dependencies": {
    "give-me-a-joke": "^0.5.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    ".scss": [
      "prettier --write"
    ]
  }
}
